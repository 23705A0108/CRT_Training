def length(array):
    count=0
    for i in array:
        count=count+1
    return count
def minimum(array):
    mini=array[0]
    for i in range(1,len(array)):
        if array[i]<mini:
            mini=array[i]
    return mini
def maximum(array):
    max=array[0]
    for i in range(1,len(array)):
        if array[i]>max:
            max=array[i]
    return max
array=list(map(int,input().split()))    
print(length(array))
print(minimum(array))
print(maximum(array))